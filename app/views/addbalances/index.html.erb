<p id="notice"><%= notice %></p>

<h1>Addbalances</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addbalances.each do |addbalance| %>
      <tr>
        <td><%= addbalance.user_id %></td>
        <td><%= addbalance.amount %></td>
        <td><%= link_to 'Show', addbalance %></td>
        <td><%= link_to 'Edit', edit_addbalance_path(addbalance) %></td>
        <td><%= link_to 'Destroy', addbalance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Addbalance', new_addbalance_path %>
